{
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": "-- Grafana --",
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "editable": true,
  "gnetId": null,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "panels": [
    {
      "datasource": "VictoriaMetrics",
      "description": "Shows all firing YouTube Downloader alerts",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "align": "left",
            "displayMode": "auto"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "severity"
            },
            "properties": [
              {
                "id": "custom.displayMode",
                "value": "color-background"
              },
              {
                "id": "mappings",
                "value": [
                  {
                    "options": {
                      "critical": {
                        "color": "red",
                        "index": 0
                      },
                      "warning": {
                        "color": "orange",
                        "index": 1
                      },
                      "info": {
                        "color": "blue",
                        "index": 2
                      }
                    },
                    "type": "value"
                  }
                ]
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "showHeader": true,
        "sortBy": []
      },
      "pluginVersion": "8.0.0",
      "targets": [
        {
          "expr": "ALERTS{alertname=~\"YT.*\"}",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "title": "Active YouTube Downloader Alerts",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true,
              "Value": true,
              "__name__": true
            },
            "indexByName": {},
            "renameByName": {
              "alertname": "Alert Name",
              "alertstate": "State",
              "severity": "Severity"
            }
          }
        }
      ],
      "type": "table"
    },
    {
      "datasource": "VictoriaMetrics",
      "description": "Indicates if service hasn't run in >48 hours",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [
            {
              "options": {
                "0": {
                  "color": "green",
                  "index": 0,
                  "text": "Healthy"
                },
                "1": {
                  "color": "red",
                  "index": 1,
                  "text": "STALE"
                }
              },
              "type": "value"
            }
          ],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 1
              }
            ]
          },
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 0,
        "y": 8
      },
      "id": 2,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "text": {},
        "textMode": "auto"
      },
      "pluginVersion": "8.0.0",
      "targets": [
        {
          "expr": "(time() - yt_last_run_timestamp) > 172800",
          "refId": "A"
        }
      ],
      "title": "Service Staleness (>48h)",
      "type": "stat"
    },
    {
      "datasource": "VictoriaMetrics",
      "description": "Current error rate with 20% threshold indicator",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "mappings": [],
          "max": 100,
          "min": 0,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "#73BF69",
                "value": null
              },
              {
                "color": "#FADE2A",
                "value": 10
              },
              {
                "color": "#F2495C",
                "value": 20
              }
            ]
          },
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 8,
        "y": 8
      },
      "id": 3,
      "options": {
        "orientation": "auto",
        "reduceOptions": {
          "values": false,
          "calcs": ["lastNotNull"],
          "fields": ""
        },
        "showThresholdLabels": true,
        "showThresholdMarkers": true,
        "text": {}
      },
      "pluginVersion": "8.0.0",
      "targets": [
        {
          "expr": "sum(rate(yt_downloads_total{status=\"failed\"}[1h])) / sum(rate(yt_downloads_total[1h])) * 100",
          "refId": "A"
        }
      ],
      "title": "Error Rate vs 20% Threshold",
      "type": "gauge"
    },
    {
      "datasource": "VictoriaMetrics",
      "description": "Channels with 3+ consecutive failures and no successes in 72h",
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "custom": {
            "align": "left",
            "displayMode": "auto"
          },
          "mappings": [],
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "color": "green",
                "value": null
              },
              {
                "color": "red",
                "value": 3
              }
            ]
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Failures"
            },
            "properties": [
              {
                "id": "custom.displayMode",
                "value": "color-background"
              }
            ]
          }
        ]
      },
      "gridPos": {
        "h": 6,
        "w": 8,
        "x": 16,
        "y": 8
      },
      "id": 4,
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "desc": true,
            "displayName": "Failures"
          }
        ]
      },
      "pluginVersion": "8.0.0",
      "targets": [
        {
          "expr": "sum by(channel) (increase(yt_downloads_total{status=\"failed\"}[72h])) >= 3 and sum by(channel) (increase(yt_downloads_total{status=\"success\"}[72h])) == 0",
          "format": "table",
          "instant": true,
          "refId": "A"
        }
      ],
      "title": "Critical Channels (3+ Failures, 72h)",
      "transformations": [
        {
          "id": "organize",
          "options": {
            "excludeByName": {
              "Time": true
            },
            "indexByName": {},
            "renameByName": {
              "Value": "Failures",
              "channel": "Channel"
            }
          }
        }
      ],
      "type": "table"
    }
  ],
  "refresh": "1m",
  "schemaVersion": 27,
  "style": "dark",
  "tags": ["youtube-downloader", "alerts", "monitoring"],
  "templating": {
    "list": []
  },
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {},
  "timezone": "browser",
  "title": "YouTube Downloader - Alerts",
  "uid": "yt-downloader-alerts",
  "version": 1
}
